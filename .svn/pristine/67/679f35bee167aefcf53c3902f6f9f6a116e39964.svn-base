# -*- coding: utf-8 -*-

import copy

def makeVo(data, oo):	
	
	if isinstance(data, list)== True:
		for index, r in enumerate(data):
			vo= copy.deepcopy(oo) 
			for k, v in r.items():
				#vo.k= v
				vo.__setattr__(k,v)
			data[index]= vo
		return data
	elif isinstance(data, dict)== True:
		vo= copy.deepcopy(oo) 
		for k, v in data.items():
			vo.__setattr__(k,v)
		return vo
	else:
		return None

